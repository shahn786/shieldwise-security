<!-- Service-Specific Schema -->
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Service",
  "serviceType": "<%= service.serviceType %> Security",
  "name": "<%= service.serviceTitle %>",
  "alternateName": "<%= service.serviceAltName %>",
  "description": "Professional <%= service.serviceType %> security services throughout California, including 24/7 security guards, access control, and mobile patrols.",
  "provider": {
    "@type": "Organization",
    "name": "ShieldWise Security Services",
    "legalName": "ShieldWise Security Services Inc."
  },
  "areaServed": [
    { "@type": "State", "name": "California" },
    { "@type": "City", "name": "Anaheim" },
    { "@type": "City", "name": "Brea" },
    { "@type": "City", "name": "Buena Park" },
    { "@type": "City", "name": "Costa Mesa" },
    { "@type": "City", "name": "Cypress" },
    { "@type": "City", "name": "Dana Point" },
    { "@type": "City", "name": "Fountain Valley" },
    { "@type": "City", "name": "Fullerton" },
    { "@type": "City", "name": "Garden Grove" },
    { "@type": "City", "name": "Huntington Beach" },
    { "@type": "City", "name": "Irvine" },
    { "@type": "City", "name": "La Habra" },
    { "@type": "City", "name": "La Palma" },
    { "@type": "City", "name": "Laguna Beach" },
    { "@type": "City", "name": "Laguna Hills" },
    { "@type": "City", "name": "Laguna Niguel" },
    { "@type": "City", "name": "Laguna Woods" },
    { "@type": "City", "name": "Lake Forest" },
    { "@type": "City", "name": "Los Alamitos" },
    { "@type": "City", "name": "Mission Viejo" },
    { "@type": "City", "name": "Newport Beach" },
    { "@type": "City", "name": "Orange" },
    { "@type": "City", "name": "Placentia" },
    { "@type": "City", "name": "Rancho Santa Margarita" },
    { "@type": "City", "name": "San Clemente" },
    { "@type": "City", "name": "San Juan Capistrano" },
    { "@type": "City", "name": "Santa Ana" },
    { "@type": "City", "name": "Seal Beach" },
    { "@type": "City", "name": "Stanton" },
    { "@type": "City", "name": "Tustin" },
    { "@type": "City", "name": "Villa Park" },
    { "@type": "City", "name": "Westminster" },
    { "@type": "City", "name": "Yorba Linda" },
    { "@type": "City", "name": "Los Angeles" },
    { "@type": "City", "name": "San Diego" },
    { "@type": "City", "name": "San Francisco" },
    { "@type": "City", "name": "Sacramento" },
    { "@type": "City", "name": "Oakland" },
    { "@type": "City", "name": "San Jose" }
  ],
  "serviceOutput": "<%= service.serviceOutput %>",
  "termsOfService": "https://shieldwisesecurity.com/terms/",
  "hoursAvailable": {
    "@type": "OpeningHoursSpecification",
    "dayOfWeek": [
      "Monday",
      "Tuesday",
      "Wednesday",
      "Thursday",
      "Friday",
      "Saturday",
      "Sunday"
    ],
    "opens": "00:00",
    "closes": "23:59"
  },
  "audience": {
    "@type": "Audience",
    "audienceType": "<%= service.audienceType %>"
  },
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "<%= service.serviceTitle %>",
    "itemListElement": [
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "24/7 Security Guards",
          "description": "Professional on-site security personnel for <%= service.serviceType %>"
        },
        "price": "<%= service.priceRange.low %>",
        "priceCurrency": "USD",
        "priceSpecification": {
          "@type": "UnitPriceSpecification",
          "price": "<%= service.priceRange.low %>",
          "priceCurrency": "USD",
          "unitText": "HOUR"
        }
      },
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Professional Security Services",
          "description": "<%= service.serviceBenefit %>"
        },
        "price": "<%= service.priceRange.mid %>",
        "priceCurrency": "USD",
        "priceSpecification": {
          "@type": "UnitPriceSpecification",
          "price": "<%= service.priceRange.mid %>",
          "priceCurrency": "USD",
          "unitText": "HOUR"
        }
      },
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Emergency Response Team",
          "description": "Rapid response security team for emergency situations"
        },
        "price": "<%= service.priceRange.high %>",
        "priceCurrency": "USD",
        "priceSpecification": {
          "@type": "UnitPriceSpecification",
          "price": "<%= service.priceRange.high %>",
          "priceCurrency": "USD",
          "unitText": "HOUR"
        }
      }
    ]
  }
}
</script>