    <!-- Core Web Vitals Optimization Script -->
    <script>
    document.addEventListener("DOMContentLoaded", function() {
        // Performance optimization for Core Web Vitals
        if ('PerformanceObserver' in window) {
            // LCP Optimization
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    if (entry.startTime < 2500) {
                        console.log('LCP optimal:', entry.startTime);
                    }
                }
            }).observe({entryTypes: ['largest-contentful-paint']});

            // CLS Optimization
            let clsValue = 0;
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    if (!entry.hadRecentInput) {
                        clsValue += entry.value;
                    }
                }
                if (clsValue < 0.1) {
                    console.log('CLS optimal:', clsValue);
                }
            }).observe({entryTypes: ['layout-shift']});

            // FID/INP Optimization
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    const inp = entry.processingStart - entry.startTime;
                    if (inp < 200) {
                        console.log('INP optimal:', inp);
                    }
                }
            }).observe({entryTypes: ['first-input']});
        }

        // Intersection Observer for animations
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        // Observe elements
        document.querySelectorAll('.service-item, .testimonial, .dtla_security_card').forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(30px)';
            el.style.transition = 'all 0.6s ease-out';
            observer.observe(el);
        });
    });
    </script>

