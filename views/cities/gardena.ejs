                    .then(registration => console.log('SW registered'))
                    .catch(error => console.log('SW registration failed'));
            });
        }

        // Enhanced Core Web Vitals Monitoring & Optimization
        (function() {
            // LCP monitoring with performance optimization
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    console.log('LCP:', entry.startTime);
                    // Report to analytics if LCP > 2.5s
                    if (entry.startTime > 2500) {
                        console.warn('LCP optimization needed - current:', entry.startTime);
                    }
                }
            }).observe({entryTypes: ['largest-contentful-paint']});

            // FID monitoring
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    console.log('FID:', entry.processingStart - entry.startTime);
                }
            }).observe({entryTypes: ['first-input']});

            // CLS monitoring with threshold alerts
            let clsValue = 0;
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    if (!entry.hadRecentInput) {
                        clsValue += entry.value;
                    }
                }
                console.log('CLS:', clsValue);
                // Alert if CLS exceeds recommended threshold
                if (clsValue > 0.1) {
                    console.warn('CLS optimization needed - current:', clsValue);
                }
            }).observe({entryTypes: ['layout-shift']});

            // INP monitoring (replacing FID in 2024)
            new PerformanceObserver((entryList) => {
                for (const entry of entryList.getEntries()) {
                    console.log('INP:', entry.duration);
                }
            }).observe({entryTypes: ['event']});
        })();
    </script>
</body>
</html>