                event.preventDefault();
                $('html, body').animate({
                    scrollTop: $($.attr(this, 'href')).offset().top - 100
                }, 800);
            });
        });

        // Initialize back to top
        if (typeof initBackToTop === 'function') {
            initBackToTop();
        }
    </script>
</body>
</html>
