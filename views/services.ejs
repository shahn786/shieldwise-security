                    "apartment-security": "mobile-patrol",
                    "special-event": "event-security"
                };

                let targetId = window.location.hash.substring(1);

                // Use fallback mapping if the target ID doesn't exist in the document
                if (targetId && !document.getElementById(targetId)) {
                    targetId = fallbackMappings[targetId] || targetId;
                }

                const targetElement = document.getElementById(targetId);
                if (targetElement) {
                    setTimeout(() => {
                        window.scrollTo({
