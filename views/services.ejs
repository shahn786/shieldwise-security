                // Use fallback mapping if the target ID doesn't exist in the document
                if (targetId && !document.getElementById(targetId)) {
                    targetId = fallbackMappings[targetId] || targetId;
                }

                const targetElement = document.getElementById(targetId);
                if (targetElement) {
                    setTimeout(() => {
                        window.scrollTo({
                            top: targetElement.offsetTop - 80, // Adjust for fixed headers if needed
                            behavior: "smooth"
                        });
                    }, 200); // Delay ensures smooth scrolling after page loads
                }
            });
        </script>
