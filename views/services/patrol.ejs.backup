<!DOCTYPE html>
<html lang="en" itemscope itemtype="https://schema.org/WebPage">
<head>
    <%- include('../partials/meta-tags-service', {
        serviceData: serviceData,
        pageTitle: serviceData.pageTitle,
        pageDescription: serviceData.pageDescription,
        canonicalUrl: serviceData.canonicalUrl
    }) %>

    <!-- Enhanced SEO Meta Tags -->
    <meta name="keywords" content="mobile patrol security, 24/7 patrol services, security patrol officers, GPS tracked patrols, rapid response security, California mobile security, alarm response patrol, construction site security patrol, property inspection services, professional security patrol">
    <meta name="robots" content="index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1">
    <meta name="googlebot" content="index, follow">
    <meta name="bingbot" content="index, follow">
    <meta name="author" content="ShieldWise Security">
    <meta name="publisher" content="ShieldWise Security">

    <!-- Geo-targeting -->
    <meta name="geo.region" content="US-CA">
    <meta name="geo.placename" content="California">

    <!-- Open Graph Enhanced -->
    <meta property="og:type" content="website">
    <meta property="og:title" content="<%= serviceData.pageTitle %> | ShieldWise Security">
    <meta property="og:description" content="<%= serviceData.pageDescription %>">
    <meta property="og:url" content="<%= serviceData.canonicalUrl %>">
    <meta property="og:site_name" content="ShieldWise Security">
    <meta property="og:image" content="https://shieldwisesecurity.com/img/mobilesecurity.webp">
    <meta property="og:locale" content="en_US">

    <!-- Twitter Card Enhanced -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= serviceData.pageTitle %>">
    <meta name="twitter:description" content="<%= serviceData.pageDescription %>">
    <meta name="twitter:image" content="https://shieldwisesecurity.com/img/mobilesecurity.webp">

    <%- include('../partials/performance-optimization') %>
    <%- include('../partials/mobile-app-meta') %>

    <!-- CSS Resources -->
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
    <link href="/css/apartment-security.css" rel="stylesheet">

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800&family=Playfair+Display:wght@400;500;600;700&display=swap" rel="stylesheet">

    <%- include('../partials/schema-primary') %>
    <%- include('../partials/schema-service', { serviceData: serviceData }) %>
    <%- include('../partials/schema-faq-service', { serviceData: serviceData }) %>
    <%- include('../partials/tracking-scripts', {
        serviceType: serviceData.serviceType,
        priceRange: serviceData.priceRange
    }) %>

    <!-- Enhanced Critical CSS Inline -->
    <style>
        :root {
            --primary-color: #1a3c6e;
            --primary-dark: #0d2b54;
            --secondary-color: #bf9e5b;
            --secondary-dark: #a88b4a;
            --text-color: #333333;
            --text-light: #666666;
            --light-bg: #f8f9fa;
            --white: #ffffff;
            --success: #28a745;
            --info: #17a2b8;
            --gradient-primary: linear-gradient(135deg, #1a3c6e 0%, #0d2b54 100%);
            --gradient-secondary: linear-gradient(135deg, #bf9e5b 0%, #a88b4a 100%);
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Inter', 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;
            color: var(--text-color);
            line-height: 1.7;
            overflow-x: hidden;
        }

        /* Hero Section */
        .hero-section {
            background: linear-gradient(rgba(26, 60, 110, 0.92), rgba(13, 43, 84, 0.92)), url('/img/mobilesecurity.webp');
            background-size: cover;
            background-position: center;
            background-attachment: fixed;
            padding: 180px 0 120px;
            color: white;
            text-align: center;
            position: relative;
            overflow: hidden;
        }

        .hero-section::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: radial-gradient(circle at 20% 50%, rgba(191, 158, 91, 0.1) 0%, transparent 50%);
            animation: pulse 15s ease-in-out infinite;
        }

        @keyframes pulse {
            0%, 100% { opacity: 0.3; }
            50% { opacity: 0.6; }
        }

        .hero-content {
            position: relative;
            z-index: 2;
        }

        .hero-badge {
            display: inline-block;
            background: rgba(191, 158, 91, 0.2);
            border: 2px solid var(--secondary-color);
            padding: 8px 24px;
            border-radius: 50px;
            font-size: 0.9rem;
            font-weight: 600;
            letter-spacing: 1px;
            text-transform: uppercase;
            margin-bottom: 25px;
            animation: slideDown 0.8s ease-out;
        }

        @keyframes slideDown {
            from {
                opacity: 0;
                transform: translateY(-30px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .hero-title {
            font-family: 'Playfair Display', serif;
            font-size: 4.2rem;
            font-weight: 700;
            margin-bottom: 25px;
            text-shadow: 3px 3px 6px rgba(0,0,0,0.4);
            line-height: 1.2;
            animation: fadeInUp 1s ease-out 0.2s both;
        }

        .hero-subtitle {
            font-size: 1.4rem;
            max-width: 850px;
            margin: 0 auto 40px;
            opacity: 0.95;
            font-weight: 300;
            line-height: 1.8;
            animation: fadeInUp 1s ease-out 0.4s both;
        }

        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(40px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }

        .hero-buttons {
            display: flex;
            gap: 20px;
            justify-content: center;
            flex-wrap: wrap;
            animation: fadeInUp 1s ease-out 0.6s both;
        }

        .btn-hero-primary {
            background: var(--gradient-secondary);
            color: white;
            padding: 18px 45px;
            border-radius: 50px;
            font-weight: 600;
            font-size: 1.1rem;
            transition: all 0.4s ease;
            border: 2px solid transparent;
            box-shadow: 0 8px 25px rgba(191, 158, 91, 0.3);
            display: inline-flex;
            align-items: center;
            gap: 12px;
            text-decoration: none;
        }

        .btn-hero-primary:hover {
            transform: translateY(-3px) scale(1.03);
            box-shadow: 0 12px 35px rgba(191, 158, 91, 0.5);
            color: white;
            text-decoration: none;
        }

        .btn-hero-secondary {
            background: transparent;
            color: white;
            padding: 18px 45px;
            border-radius: 50px;
            font-weight: 600;
            font-size: 1.1rem;
            transition: all 0.4s ease;
            border: 2px solid white;
            display: inline-flex;
            align-items: center;
            gap: 12px;
            text-decoration: none;
        }

        .btn-hero-secondary:hover {
            background: white;
            color: var(--primary-color);
            transform: translateY(-3px);
            text-decoration: none;
        }

        /* Trust Indicators */
        .trust-indicators {
            background: white;
            padding: 30px 0;
            box-shadow: 0 -5px 20px rgba(0,0,0,0.05);
            position: relative;
            z-index: 10;
        }

        .trust-item {
            text-align: center;
            padding: 15px;
        }

        .trust-icon {
            font-size: 2.5rem;
            color: var(--secondary-color);
            margin-bottom: 12px;
            display: block;
        }

        .trust-text {
            font-size: 1.1rem;
            font-weight: 600;
            color: var(--primary-color);
            margin-bottom: 5px;
        }

        .trust-subtext {
            font-size: 0.9rem;
            color: var(--text-light);
        }

        /* Premier Overview Section */
        .premier-overview {
            background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);
            padding: 80px 0;
        }

        .overview-card {
            background: white;
            padding: 60px;
            border-radius: 20px;
            box-shadow: 0 15px 50px rgba(0,0,0,0.08);
            border: 1px solid #e8e8e8;
            position: relative;
            overflow: hidden;
        }

        .overview-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 5px;
            height: 100%;
            background: var(--gradient-secondary);
        }

        .overview-title {
            font-family: 'Playfair Display', serif;
            font-size: 2.8rem;
            margin-bottom: 30px;
            color: var(--primary-dark);
            font-weight: 700;
        }

        .overview-text {
            font-size: 1.15rem;
            line-height: 1.9;
            margin-bottom: 25px;
            color: var(--text-color);
        }

        .highlight-badge {
            background: var(--gradient-secondary);
            color: white;
            padding: 4px 12px;
            border-radius: 6px;
            font-weight: 600;
            font-size: 0.95rem;
        }

        .contact-info-box {
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
            padding: 25px 30px;
            border-radius: 12px;
            margin-top: 30px;
            border-left: 4px solid var(--secondary-color);
        }

        .contact-info-box strong {
            color: var(--primary-color);
        }

        /* Service Cards */
        .service-card {
            background: white;
            border-radius: 20px;
            padding: 40px;
            margin: 20px 0;
            box-shadow: 0 10px 40px rgba(0,0,0,0.08);
            transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
            border: 1px solid #f0f0f0;
            position: relative;
            overflow: hidden;
        }

        .service-card::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 4px;
            background: var(--gradient-secondary);
            transform: scaleX(0);
            transition: transform 0.4s ease;
        }

        .service-card:hover {
            transform: translateY(-10px) scale(1.02);
            box-shadow: 0 20px 60px rgba(26, 60, 110, 0.15);
        }

        .service-card:hover::before {
            transform: scaleX(1);
        }

        .service-icon {
            font-size: 3.5rem;
            background: var(--gradient-secondary);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 25px;
            display: inline-block;
        }

        .service-title {
            color: var(--primary-color);
            font-size: 1.9rem;
            margin-bottom: 20px;
            font-weight: 700;
            font-family: 'Playfair Display', serif;
        }

        .service-description {
            color: var(--text-light);
            font-size: 1.05rem;
            margin-bottom: 25px;
            line-height: 1.8;
        }

        .feature-list {
            list-style: none;
            padding: 0;
        }

        .feature-list li {
            padding: 14px 0;
            padding-left: 35px;
            position: relative;
            font-size: 1.05rem;
            color: var(--text-color);
            transition: all 0.3s ease;
        }

        .feature-list li:before {
            content: 'âœ“';
            position: absolute;
            left: 0;
            top: 14px;
            width: 24px;
            height: 24px;
            background: var(--gradient-secondary);
            color: white;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-weight: bold;
            font-size: 0.8rem;
        }

        .feature-list li:hover {
            padding-left: 40px;
            color: var(--primary-color);
        }

        /* Stats Section */
        .stats-section {
            padding: 90px 0;
            background: var(--gradient-primary);
            color: white;
            position: relative;
            overflow: hidden;
        }

        .stats-section::before {
            content: '';
            position: absolute;
            top: -50%;
            right: -10%;
            width: 600px;
            height: 600px;
            background: radial-gradient(circle, rgba(191, 158, 91, 0.1) 0%, transparent 70%);
            border-radius: 50%;
        }

        .stat-card {
            text-align: center;
            padding: 40px 20px;
            position: relative;
            z-index: 2;
        }

        .stat-icon {
            font-size: 3rem;
            color: var(--secondary-color);
            margin-bottom: 20px;
            display: block;
        }

        .stat-number {
            font-size: 3.5rem;
            font-weight: 800;
            color: white;
            margin-bottom: 12px;
            text-shadow: 2px 2px 4px rgba(0,0,0,0.2);
            font-family: 'Inter', sans-serif;
        }

        .stat-label {
            font-size: 1.1rem;
            opacity: 0.95;
            font-weight: 500;
            letter-spacing: 0.5px;
        }

        /* Patrol Coverage Section */
        .patrol-coverage {
            background: white;
            padding: 90px 0;
        }

        .section-header {
            text-align: center;
            margin-bottom: 70px;
        }

        .section-subtitle-badge {
            display: inline-block;
            background: rgba(191, 158, 91, 0.1);
            color: var(--secondary-color);
            padding: 8px 20px;
            border-radius: 50px;
            font-size: 0.9rem;
            font-weight: 600;
            letter-spacing: 1px;
            text-transform: uppercase;
            margin-bottom: 20px;
        }

        .section-title {
            font-family: 'Playfair Display', serif;
            font-size: 3rem;
            color: var(--primary-dark);
            margin-bottom: 20px;
            font-weight: 700;
        }

        .section-description {
            font-size: 1.2rem;
            color: var(--text-light);
            max-width: 800px;
            margin: 0 auto;
            line-height: 1.8;
        }

        /* Technology Features */
        .tech-features {
            background: linear-gradient(135deg, #f8f9fa 0%, #e9ecef 100%);
            padding: 90px 0;
        }

        .tech-card {
            background: white;
            padding: 45px;
            border-radius: 20px;
            box-shadow: 0 10px 30px rgba(0,0,0,0.06);
            text-align: center;
            transition: all 0.4s ease;
            border: 2px solid transparent;
            height: 100%;
        }

        .tech-card:hover {
            border-color: var(--secondary-color);
            transform: translateY(-8px);
            box-shadow: 0 15px 45px rgba(0,0,0,0.12);
        }

        .tech-icon-wrapper {
            width: 100px;
            height: 100px;
            margin: 0 auto 30px;
            background: var(--gradient-secondary);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            box-shadow: 0 8px 20px rgba(191, 158, 91, 0.3);
        }

        .tech-icon {
            font-size: 2.5rem;
            color: white;
        }

        .tech-title {
            font-size: 1.6rem;
            color: var(--primary-color);
            margin-bottom: 18px;
            font-weight: 700;
        }

        .tech-description {
            font-size: 1.05rem;
            color: var(--text-light);
            line-height: 1.8;
        }

        /* Testimonials */
        .testimonials-section {
            padding: 90px 0;
            background: white;
        }

        .testimonial-card {
            background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);
            padding: 45px;
            border-radius: 20px;
            margin: 20px 0;
            box-shadow: 0 10px 35px rgba(0,0,0,0.08);
            position: relative;
            transition: all 0.4s ease;
            border: 1px solid #e8e8e8;
        }

        .testimonial-card:hover {
            transform: translateY(-8px);
            box-shadow: 0 15px 50px rgba(0,0,0,0.15);
        }

        .quote-icon {
            position: absolute;
            top: 30px;
            right: 35px;
            font-size: 4rem;
            color: rgba(191, 158, 91, 0.15);
        }

        .client-header {
            display: flex;
            align-items: center;
            margin-bottom: 25px;
        }

        .client-image {
            width: 80px;
            height: 80px;
            border-radius: 50%;
            margin-right: 20px;
            border: 3px solid var(--secondary-color);
            box-shadow: 0 5px 15px rgba(0,0,0,0.1);
        }

        .client-info h4 {
            margin: 0;
            font-size: 1.3rem;
            color: var(--primary-color);
            font-weight: 700;
        }

        .client-info .client-title {
            color: var(--text-light);
            font-size: 0.95rem;
            margin-top: 5px;
        }

        .rating {
            color: #ffc107;
            font-size: 1.2rem;
            margin-bottom: 20px;
        }

        .testimonial-text {
            font-size: 1.1rem;
            line-height: 1.9;
            color: var(--text-color);
            font-style: italic;
            position: relative;
            z-index: 2;
        }

        /* Process Timeline */
        .process-section {
            background: linear-gradient(135deg, #1a3c6e 0%, #0d2b54 100%);
            padding: 90px 0;
            color: white;
        }

        .timeline {
            position: relative;
            padding: 40px 0;
        }

        .timeline::before {
            content: '';
            position: absolute;
            top: 0;
            left: 50%;
            width: 3px;
            height: 100%;
            background: rgba(191, 158, 91, 0.3);
            transform: translateX(-50%);
        }

        .timeline-item {
            position: relative;
            margin-bottom: 60px;
        }

        .timeline-item:nth-child(odd) .timeline-content {
            margin-left: auto;
            text-align: left;
        }

        .timeline-item:nth-child(even) .timeline-content {
            margin-right: auto;
            text-align: right;
        }

        .timeline-content {
            width: 45%;
            background: rgba(255, 255, 255, 0.1);
            padding: 35px;
            border-radius: 15px;
            backdrop-filter: blur(10px);
            border: 1px solid rgba(255, 255, 255, 0.2);
        }

        .timeline-number {
            position: absolute;
            left: 50%;
            top: 0;
            transform: translateX(-50%);
            width: 60px;
            height: 60px;
            background: var(--gradient-secondary);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 1.5rem;
            font-weight: 700;
            box-shadow: 0 5px 20px rgba(191, 158, 91, 0.4);
            z-index: 2;
        }

        .timeline-title {
            font-size: 1.6rem;
            margin-bottom: 15px;
            font-weight: 700;
        }

        .timeline-description {
            font-size: 1.05rem;
            line-height: 1.8;
            opacity: 0.9;
        }

        /* CTA Section */
        .cta-section {
            background: var(--gradient-primary);
            padding: 100px 0;
            color: white;
            text-align: center;
            position: relative;
            overflow: hidden;
        }

        .cta-section::before {
            content: '';
            position: absolute;
            top: -50%;
            left: -10%;
            width: 500px;
            height: 500px;
            background: radial-gradient(circle, rgba(191, 158, 91, 0.15) 0%, transparent 70%);
            border-radius: 50%;
            animation: float 20s ease-in-out infinite;
        }

        @keyframes float {
            0%, 100% { transform: translate(0, 0); }
            50% { transform: translate(50px, 50px); }
        }

        .cta-content {
            position: relative;
            z-index: 2;
        }

        .cta-title {
            font-family: 'Playfair Display', serif;
            font-size: 3.2rem;
            margin-bottom: 25px;
            font-weight: 700;
        }

        .cta-subtitle {
            font-size: 1.3rem;
            margin-bottom: 45px;
            opacity: 0.95;
            max-width: 700px;
            margin-left: auto;
            margin-right: auto;
        }

        .cta-features {
            display: flex;
            justify-content: center;
            gap: 40px;
            flex-wrap: wrap;
            margin-bottom: 45px;
        }

        .cta-feature {
            display: flex;
            align-items: center;
            gap: 12px;
            font-size: 1.1rem;
        }

        .cta-feature i {
            color: var(--secondary-color);
            font-size: 1.3rem;
        }

        .btn-custom {
            background: var(--gradient-secondary);
            color: white;
            padding: 20px 50px;
            border-radius: 50px;
            font-weight: 700;
            font-size: 1.2rem;
            transition: all 0.4s ease;
            border: none;
            box-shadow: 0 10px 30px rgba(191, 158, 91, 0.4);
            display: inline-flex;
            align-items: center;
            gap: 15px;
            cursor: pointer;
        }

        .btn-custom:hover {
            transform: translateY(-4px) scale(1.05);
            box-shadow: 0 15px 40px rgba(191, 158, 91, 0.6);
            color: white;
        }

        .btn-custom i {
            font-size: 1.4rem;
        }

        /* FAQ Section */
        .faq-section {
            background: white;
            padding: 90px 0;
        }

        .faq-item {
            background: #f8f9fa;
            border-radius: 12px;
            margin-bottom: 20px;
            overflow: hidden;
            border: 1px solid #e0e0e0;
            transition: all 0.3s ease;
        }

        .faq-item:hover {
            border-color: var(--secondary-color);
            box-shadow: 0 5px 20px rgba(0,0,0,0.08);
        }

        .faq-question {
            padding: 25px 30px;
            cursor: pointer;
            font-size: 1.2rem;
            font-weight: 600;
            color: var(--primary-color);
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .faq-answer {
            padding: 0 30px 25px;
            font-size: 1.05rem;
            line-height: 1.8;
            color: var(--text-light);
            display: none;
        }

        .skip-link {
            position: absolute;
            top: -40px;
            left: 0;
            background: var(--primary-color);
            color: white;
            padding: 8px 16px;
            z-index: 100;
            text-decoration: none;
            border-radius: 0 0 4px 0;
        }

        .skip-link:focus {
            top: 0;
        }

        /* Responsive Design */
        @media (max-width: 1200px) {
            .hero-title {
                font-size: 3.5rem;
            }
        }

        @media (max-width: 992px) {
            .timeline::before {
                left: 30px;
            }

            .timeline-content {
                width: calc(100% - 100px);
                margin-left: 80px !important;
                text-align: left !important;
            }

            .timeline-number {
                left: 30px;
                transform: translateX(0);
            }
        }

        @media (max-width: 768px) {
            .hero-title {
                font-size: 2.8rem;
            }

            .hero-subtitle {
                font-size: 1.2rem;
            }

            .section-title {
                font-size: 2.2rem;
            }

            .overview-card {
                padding: 40px 30px;
            }

            .overview-title {
                font-size: 2.2rem;
            }

            .service-card {
                padding: 30px;
            }

            .stat-number {
                font-size: 2.5rem;
            }

            .cta-title {
                font-size: 2.2rem;
            }

            .hero-buttons {
                flex-direction: column;
                align-items: stretch;
            }

            .btn-hero-primary,
            .btn-hero-secondary {
                width: 100%;
                justify-content: center;
            }
        }

        @media (max-width: 576px) {
            .hero-section {
                padding: 120px 0 80px;
            }

            .hero-title {
                font-size: 2.2rem;
            }

            .overview-card {
                padding: 30px 20px;
            }

            .tech-card {
                padding: 30px 20px;
            }
        }
