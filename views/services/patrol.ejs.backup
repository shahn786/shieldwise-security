                            <i class="fas fa-quote-right quote-icon" aria-hidden="true"></i>
                            <div class="client-header">
                                <img src="/img/<%= testimonial.image %>" alt="<%= testimonial.name %>, <%= testimonial.title %>" class="client-image" loading="lazy" width="80" height="80" itemprop="image">
                                <div class="client-info" itemprop="author" itemscope itemtype="https://schema.org/Person">
                                    <h4 itemprop="name"><%= testimonial.name %></h4>
                                    <p class="client-title" itemprop="jobTitle"><%= testimonial.title %></p>
                                </div>
                            </div>
                            <div class="rating" itemprop="reviewRating" itemscope itemtype="https://schema.org/Rating">
